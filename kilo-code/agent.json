{
  "id": "kilo-code",
  "name": "Kilo Code",
  "version": "7.0.27",
  "description": "Kilo Code - AI coding assistant with support for 500+ AI models, multi-agent orchestration, and seamless session syncing across IDEs",
  "repository": "https://github.com/Kilo-Org/kilocode",
  "authors": [
    "Kilo Code Inc."
  ],
  "license": "Apache-2.0",
  "distribution": {
    "npx": {
      "package": "@kilocode/cli@7.0.27",
      "args": ["acp"]
    }
  },
  "mcpServers": {
    "ripgrep": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-ripgrep@latest"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/cammyz/robo-viewer"
      ],
      "env": {},
      "timeout": 60000,
      "trust": false
    },
    "git": {
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ],
      "env": {},
      "timeout": 60000,
      "trust": false
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {},
      "timeout": 30000,
      "trust": false
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {},
      "timeout": 30000,
      "trust": false
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {},
      "timeout": 120000,
      "trust": false
    },
    "time": {
      "command": "/home/cammyz/.local/bin/mcp-server-time-fixed",
      "args": [],
      "env": {},
      "timeout": 10000,
      "trust": true
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ],
      "env": {},
      "timeout": 60000,
      "trust": false
    },
    "duckdb": {
      "command": "npx",
      "args": [
        "-y",
        "duckdb-mcp"
      ],
      "env": {},
      "timeout": 60000,
      "trust": false
    },
    "everything": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-everything"
      ],
      "env": {},
      "timeout": 30000,
      "trust": false
    }
  }
}
